exit
@ip_str.chars.each{ |c| keys.each {|key| sum += @score_hash[key] if  key.downcase.include?c}}
@ip_str.chars.each{ |c| keys.each {|key| p @score_hash[key] if  key.downcase.include?c}}
@ip_str.chars.each{ |c| keys.each {|key| p key.downcase.include?c}}
@ip_str.chars.each{ |c| keys.each {|key| p key.include?c}}
@ip_str.chars.each{ |c| keys.each {|key| p key}}
@ip_str.chars.each{ |c| p c}
@ip_str.chars
@ip_str
